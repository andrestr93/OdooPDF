<?xml version="1.0" encoding="utf-8" ?>
<odoo>

    <template id="report_empleado">
        <t t-call="web.html_container">
            <t t-foreach="docs" t-as="emp">
                <t t-call="web.external_layout">
                    <h2>Reporte de empleado:</h2>
                    <t t-esc="ing.numingrediente"/>
                    <t t-esc="ing.nombre"/>
                    <t t-esc="ing.cantidad"/>
                    <t t-esc="ing.proveedores"/>
                </t>
>            </t>
        </t>
    </template>

</odoo>